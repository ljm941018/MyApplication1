<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.jae62.myapplication1.MainActivity">

    <Button
        android:text="simple_list2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/main_button_simple_list1"
        android:layout_alignEnd="@+id/main_button_simple_list1"
        android:layout_marginTop="33dp"
        android:id="@+id/main_button_simple_list2" />

    <Button
        android:text="simple_list1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="166dp"
        android:id="@+id/main_button_simple_list1"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="CUSTOM_LIST"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:id="@+id/main_button_custom_list"
        android:layout_below="@+id/main_button_simple_list2"
        android:layout_alignStart="@+id/main_button_simple_list2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="103dp" />
</RelativeLayout>
